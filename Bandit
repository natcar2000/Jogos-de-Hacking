NÍVEL 8-9
    # PROCURANDO A SENHA
    cat data.txt | sort | uniq -u 
    
    #CONEXÃO COM O HOST
    ssh -p 2220 bandit9@bandit.labs.overthewire.org
 
NÍVEL 9-10
    #PROCURANDO A SENHA
