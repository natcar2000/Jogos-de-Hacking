NÍVEL 0
    # CONEXÃO COM O HOST
    ssh -p 2220 bandit0@bandit.labs.overthewire.org
    senha: bandit0
   
NÍVEL 0-1
    #PROCURANDO A SENHA
    cd ~
    cat readme

    #CONEXÃO COM O HOST
    ssh -p 2220 bandit1@bandit.labs.overthewire.org
    
NÍVEL 1-2
    #PROCURANDO A SENHA
    cd ~
    rev - | rev  #Ignora o meta-caractere, inverte o conteúdo e depois volta para a posição correta
    
    #CONEXÃO AO HOST
    ssh -p 2220 bandit2@bandit.labs.overthewire.org 
    
NÍVEL 2-3
    #PROCURANDO A SENHA
    cd ~
    cat "spaces in this filename"  #As aspas devem ser usadas, caso contrário será exibido um erro
    
    #CONEXÃO COM O HOST
    ssh -p 2220 bandit3@bandit.labs.overthewire.org
    
NÍVEL 3-4
    #PROCURANDO A SENHA
    cd ~/inhere
    ls -a
    cat .hidden
    
    #CONEXÃO COM O HOST
    ssh -p 2220 bandit4@bandit.labs.overthewire.org
    
NÍVEL 4-5
    #PROCURANDO A SENHA
    cd ~/inhere
    cat < -file07
    
    #CONEXÃO COM O HOST
    ssh -p 2220  bandit5@bandit.labs.overthewire.org

NÍVEL 5-6
    #PROCURANDO A SENHA
    cd ~/inhere
    find -type f -size 1033c #Resultado: ./maybehere07/.file2
    cd maybehere07
    ls -a
    cat .file2
    
    #CONEXÃO COM O HOST
    ssh -p 2220 bandit6@bandit.labs.overthewire.org
    
NÍVEL 6-7
    #PROCURANDO A SENHA
    find / -user bandit7 -group bandit6 -type f -size 33c 2>/dev/null #Resultado: /var/lib/dpkg/info/bandit7.password
    cat /var/lib/dpkg/info/bandit7.password
    
    #CONEXÃO COM O HOST
    ssh -p 2220 bandit7@bandit.labs.overthewire.org

NÍVEL 7-8
    #PROCURANDO A SENHA
    cat data.txt | grep "millionth" | cut -c 11-42
    
    #CONEXÃO COM O HOST
    ssh -p 2220 bandit8@bandit.labs.overthewire.org
    
NÍVEL 8-9
    # PROCURANDO A SENHA
    cat data.txt | sort | uniq -u 
    
    #CONEXÃO COM O HOST
    ssh -p 2220 bandit9@bandit.labs.overthewire.org
 
NÍVEL 9-10
    #PROCURANDO A SENHA
    cat data.txt | grep ^"=" | grep "G7" | cut -c 12-43 
    
    #CONEXÃO COM O HOST
    ssh -p 2220 bandit10@bandit.labs.overthewire.org
    
NÍVEL 10-11
    #PROCURANDO A SENHA
